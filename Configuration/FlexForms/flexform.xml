<T3DataStructure>
	<sheets>
		<sDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:jqct/Resources/Private/Language/locallang_be.xml:flexforms_tab.settings</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					
					<settings.renderingMethod>
						<TCEforms>
							<label>LLL:EXT:jqct/Resources/Private/Language/locallang_be.xml:flexforms_settings.renderingMethod</label>
							<config>
								<type>select</type>
								<!--<itemsProcFunc>Tx_Jqct_Hook_ItemsProcFunc->user_switchableControllerActions</itemsProcFunc>-->
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0"></numIndex>
										<numIndex index="1"></numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:jqct/Resources/Private/Language/locallang_be.xml:flexforms_settings.renderingMethod.1</numIndex>
										<numIndex index="1">1</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">LLL:EXT:jqct/Resources/Private/Language/locallang_be.xml:flexforms_settings.renderingMethod.2</numIndex>
										<numIndex index="1">2</numIndex>
									</numIndex>
								</items>
								<minitems>0</minitems>
								<maxitems>1</maxitems>
								<size>1</size>
							</config>
						</TCEforms>
					</settings.renderingMethod>
					
					<settings.records>
						<TCEforms>
							<label>LLL:EXT:jqct/Resources/Private/Language/locallang_be.xml:flexforms_settings.records</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>tt_content</allowed>
								<size>5</size>
								<minitems>0</minitems>
								<maxitems>50</maxitems>
								<show_thumbs>1</show_thumbs>
								<autoSizeMax>40</autoSizeMax>
							</config>
						</TCEforms>
					</settings.records>
					
          <settings.headerOverride>
            <TCEforms>
              <label>LLL:EXT:jqct/Resources/Private/Language/locallang_be.xml:flexforms_settings.headerOverride</label>
              <config>
                <type>text</type>
                <cols>30</cols>
                <rows>5</rows>
              </config>
            </TCEforms>
          </settings.headerOverride>						

  				<settings.elementMerging>
  					<TCEforms>
  						<label>LLL:EXT:jqct/Resources/Private/Language/locallang_be.xml:flexforms_settings.elementMerging</label>
  						<config>
  							<type>input</type>
  							<size>30</size>
  						</config>
  					</TCEforms>
  				</settings.elementMerging>
					
					<settings.ajax>
						<TCEforms>
							<label>LLL:EXT:jqct/Resources/Private/Language/locallang_be.xml:flexforms_settings.ajax</label>
							<config>
								<type>check</type>
								<default>0</default>
							</config>
						</TCEforms>
					</settings.ajax>
					
					<settings.showfirst>
						<TCEforms>
							<label>LLL:EXT:jqct/Resources/Private/Language/locallang_be.xml:flexforms_settings.showfirst</label>
							<config>
								<type>input</type>
								<size>2</size>
								<eval>int</eval>
								<default>0</default>
								<!-- <checkbox>1</checkbox> -->
							</config>
						</TCEforms>
					</settings.showfirst>

				</el>
			</ROOT>
		</sDEF>
	</sheets>
</T3DataStructure>